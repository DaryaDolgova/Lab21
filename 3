using namespace std;

#include <string>
#include <windows.h>
#include <iostream>

int main() 
{

	SetConsoleCP(1251);

	SetConsoleOutputCP(1251);

	string s;
	int c,i;
	
	cout << "S=";
	getline(cin, s);

	c = s[0];

	for (i = 1; i < s.size(); i++)
	{
		if (' '!=s[i] && ' '==s[i - 1] ) { c =s[i]; }
		if (s[i] != ' ' && s[i] == c && s[i - 1] != ' ') {
			s[i] = '.';
		}
	}

	cout << s << endl;

	return 0;
}
