#include <iostream>
#include <windows.h>
#include <string>

using namespace std;

int main()

{
	char ch(92);
	string S;
	int  i, n, w, e;

	cout << "S=\n";
	getline(cin, S);
	e = 0;
	for (i = S.size(); i > 0; i--)
		if (S[i] == '.')
		{
			w = i - 1;
			for ( int j = i + 1; S[j] != ch; j--)
			{
				e++;
				n = j;
			}
		}

	for (int i = n; i <= w; i++) 
	{
		cout << S[i];
	}
	return 0;
}
